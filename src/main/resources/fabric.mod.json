{
  "schemaVersion": 1,
  "id": "fabric-spigot",
  "version": "${version}",
  "name": "Fabric Spigot",
  "description": "",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/fabric-spigot/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.gaming32.fabricspigot.FabricSpigot"
    ],
    "preLaunch": [
      "io.github.gaming32.fabricspigot.FabricSpigotPreLaunch"
    ]
  },
  "mixins": [
    "fabric-spigot.mixins.json"
  ],
  "accessWidener": "fabric-spigot.accessWidener",
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_2561": ["io/github/gaming32/fabricspigot/util/TextStreamable"],
      "net/minecraft/server/MinecraftServer": ["io/github/gaming32/fabricspigot/vanillaimpl/HasBukkitServer"],
      "net/minecraft/class_3218": ["io/github/gaming32/fabricspigot/vanillaimpl/HasBukkitWorld"],
      "net/minecraft/class_2168": ["io/github/gaming32/fabricspigot/vanillaimpl/BukkitServerCommandSource"],
      "net/minecraft/class_2165": ["io/github/gaming32/fabricspigot/vanillaimpl/CommandOutputExt"]
    }
  },
  "depends": {
    "fabricloader": ">=0.14.12",
    "fabric": "*",
    "minecraft": "1.19.3",
    "fabric-permissions-api-v0": "*"
  }
}
